--- /dev/null	(date 1715126927608)
+++ b/src/ContaBancaria.java	(date 1715126927608)
@@ -0,0 +1,19 @@
+public class ContaBancaria {
+    private double saldo;
+
+    public double getSaldo() {
+        return saldo;
+    }
+
+    public void sacar(double valor) throws SaldoInsuficienteException {
+        if(valor > saldo){
+            String msg = String.format("Saldo insuficiente: Tentativa de saque de R$ %.2f com saldo de R$ %.2f", valor, saldo);
+            throw new SaldoInsuficienteException(msg);
+        }
+        this.saldo -= valor;
+    }
+
+    public void depositar(double valor){
+        saldo += valor;
+    }
+}
